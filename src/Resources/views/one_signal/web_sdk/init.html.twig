<script src="https://cdn.onesignal.com/sdks/OneSignalSDK.js" async='async'></script>
<script>
    var OneSignal = window.OneSignal || [];
    OneSignal.push(["init", {
      appId: "{{ dmytrof_push_notification_parameter('one_signal.app_id') }}",
      autoRegister: true, /* Set to true to automatically prompt visitors */
      {% if dmytrof_push_notification_parameter('one_signal.subdomain') %}subdomainName: "{{ dmytrof_push_notification_parameter('one_signal.subdomain') }}",{% endif %}
      httpPermissionRequest: {
        enable: true
      },
      notifyButton: {
          enable: true /* Set to false to hide */
      }
    }]);

    {{ dmytrof_push_notification_tags() }}

</script>
